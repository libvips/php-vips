{
    "name": "jcupitt/vips",
    "description": "A high-level interface to the libvips image processing library.",
    "keywords": [
        "image",
        "processing",
        "libvips"
    ],
    "homepage": "https://github.com/libvips/php-vips",
    "license": "MIT",
    "authors": [
        {
            "name": "John Cupitt",
            "email": "jcupitt@gmail.com",
            "homepage": "https://github.com/jcupitt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/log": "^1.1.3"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpdocumentor/phpdocumentor": "3.0.0",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Jcupitt\\Vips\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jcupitt\\Vips\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit",
            "phpcs --standard=phpcs-ruleset.xml ."
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
